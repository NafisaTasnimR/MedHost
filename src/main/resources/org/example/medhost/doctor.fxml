<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.medhost.DoctorController">
   <children>
      <VBox prefHeight="600.0" prefWidth="700.0">
         <children>
            <Pane prefHeight="65.0" prefWidth="700.0">
               <children>
                  <Button fx:id="logoutButton" layoutX="607.0" layoutY="14.0" mnemonicParsing="false" onAction="#logOutButtonHandler" prefHeight="31.0" prefWidth="69.0" text="LogOut" />
               </children>
            </Pane>
            <Pane prefHeight="469.0" prefWidth="700.0">
               <children>
                  <Button fx:id="scheduledAppointment" layoutX="250.0" layoutY="90.0" mnemonicParsing="false" onAction="#appointmentActionHandler" prefHeight="90.0" prefWidth="185.0" text="Appointment">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font></Button>
                  <Button fx:id="patientInformationButton" layoutX="250.0" layoutY="251.0" mnemonicParsing="false" onAction="#patientInformationActionHandler" prefHeight="90.0" prefWidth="185.0" text="Patient  Information">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font></Button>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
