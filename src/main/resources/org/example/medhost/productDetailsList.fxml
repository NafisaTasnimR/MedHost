<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.medhost.ProductDetailsListController">
    <children>
        <VBox layoutY="1.0" prefHeight="560.0" prefWidth="700.0">
            <children>
                <Pane prefHeight="53.0" prefWidth="700.0">
                    <children>
                        <Button fx:id="homeButton" layoutX="20.0" layoutY="14.0" mnemonicParsing="false" onAction="#homeButtonHandler" prefHeight="27.0" prefWidth="63.0" text="Home">
                            <font>
                                <Font name="System Bold" size="13.0" />
                            </font>
                        </Button>
                        <Button fx:id="logOutButton" layoutX="604.0" layoutY="14.0" mnemonicParsing="false" onAction="#logOutButtonHandler" text="LogOut">
                            <font>
                                <Font name="System Bold" size="13.0" />
                            </font>
                        </Button>
                    </children>
                </Pane>
                <Pane prefHeight="80.0" prefWidth="700.0">
                    <children>
                        <TextField fx:id="searchTextField" layoutX="130.0" layoutY="21.0" prefHeight="25.0" prefWidth="294.0" promptText="search" />
                        <Button fx:id="searchButton" layoutX="444.0" layoutY="16.0" mnemonicParsing="false" onAction="#searchButtonActionHandler" prefHeight="35.0" prefWidth="80.0" text="Search">
                            <font>
                                <Font name="System Bold" size="13.0" />
                            </font>
                        </Button>
                    </children>
                </Pane>
                <Pane prefHeight="382.0" prefWidth="700.0">
                    <children>
                  <Label layoutX="156.0" layoutY="89.0" text="Item No.">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <Label layoutX="139.0" layoutY="125.0" text="Product Name">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <Label layoutX="158.0" layoutY="169.0" text="Supplier">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <Label layoutX="141.0" layoutY="214.0" text="Current Stock">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <Label layoutX="153.0" layoutY="256.0" text="DOC Date">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <Label layoutX="148.0" layoutY="301.0" text="Expire Date">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <Text fx:id="itemNoText" layoutX="259.0" layoutY="103.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false" />
                  <Text fx:id="productNameText" layoutX="259.0" layoutY="139.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false" />
                  <Text fx:id="supplierText" layoutX="259.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false" />
                  <Text fx:id="currentStockText" layoutX="259.0" layoutY="228.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false" />
                  <Text fx:id="docDateText" layoutX="259.0" layoutY="270.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false" />
                  <Text fx:id="expireDateText" layoutX="259.0" layoutY="315.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false" />
                  <Label layoutX="237.0" layoutY="14.0" text="Product Details">
                     <font>
                        <Font name="System Bold Italic" size="24.0" />
                     </font>
                  </Label>
                    </children>
                </Pane>
            </children>
        </VBox>
    </children>
</AnchorPane>
