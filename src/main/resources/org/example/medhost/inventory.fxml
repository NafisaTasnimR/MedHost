<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.medhost.InventoryController">
   <children>
      <VBox layoutY="2.0" prefHeight="600.0" prefWidth="700.0">
         <children>
            <Pane prefHeight="58.0" prefWidth="700.0">
               <children>
                  <Button fx:id="homeButton" layoutX="21.0" layoutY="17.0" mnemonicParsing="false" onAction="#homeButtonHandler" prefHeight="25.0" prefWidth="67.0" text="Home">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="logOutButton" layoutX="619.0" layoutY="17.0" mnemonicParsing="false" onAction="#logOutButtonHandler" prefHeight="25.0" prefWidth="67.0" text="LogOut">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
            <Pane prefHeight="453.0" prefWidth="700.0">
               <children>
                  <Button fx:id="listOfProductButton" layoutX="239.0" layoutY="109.0" mnemonicParsing="false" onAction="#listOfProductButtonHandler" prefHeight="108.0" prefWidth="196.0" text="List Of Products">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="productDetailsListButton" layoutX="240.0" layoutY="272.0" mnemonicParsing="false" onAction="#productDetailsListButtonHandler" prefHeight="108.0" prefWidth="196.0" text="Product Details List">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
